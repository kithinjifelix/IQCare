    <script type="text/javascript">
        $(document).ready(function() {

            var startEncounter = 0; /* starts encounter flags starts at false */
            var stopEncounter = 0; /* starts encounter flags starts at false */
         

            var patientId = <%=PatientId%>;

            if (patientId > 0) {
                $("#editPatientDetails").show();
            } else {
                $("#editPatientDetails").hide();
            }

            $("#menuStartEncounter").click(function() {
                if (patientId < 1) {
                    toastr.error("No Patient Selected to start Encounter", "==Patient Encounter Start Alert==");
                    return false;
                };
                if (startEncounter > 0) {
                    toastr.error("Please stop an existing encounter first", "Encounter Start Alert");
                    return false;
                } else {
                   
                   // toastr.info("Encounter Start Activated", "Encounter Start Alert");
                    encounterStarted();
                    
                    $(this).hide();
                    startEncounter = 1; /* denotes an active encounter */
                    stopEncounter = 0;
                    toastr.info("Encounter Start Successfully", "Encounter Start Alert");
                };

            });

            $("#menuStopEncounter").click(function() {
                if (startEncounter < 0) {
                    toastr.error("No Active Encounter to Stop,", "Encounter Stop Alert");
                    return false;
                } else {
                    toastr.error("Encounter Stop Activated", "Encounter Stop Alert");
                    encounterStopped();
                    stopEncounter = 1;
                    startEncounter = 0;
                    toastr.info("Encounter Stopped Successfully", "Encounter Start Alert");
                }

            });

            if(startEncounter===0 & stopEncounter===0){   encounterStopped();}
            else if (startEncounter > 0 & patientId > 0) {
                //encounterStopped();
                encounterStarted();
            }

            function encounterStopped() {

                $("#titlePatientBaseline").hide();
                $("#menuBaseline").hide();
                $("#menuFamilyTesting").hide();
                $("#MenuOneTimeEventsTracker").hide();
                $("#menuStopEncounter").hide();

                $("#menuPatientHome").hide();
                $("#menuPatientAppointments").hide();
                $("#menuScheduleAppointments").hide();
                $("#menuCaptureVitals").hide();
                $("#menuNewEncounter").hide();
                $("#menuPatientVitals").hide();
                <%-- // window.open("<%=ResolveUrl("patient/patientfinder.aspx")%>");--%>
            }

            function encounterStarted() {
                $("#titlePatientBaseline").show();
                $("#menuBaseline").show();
                $("#menuFamilyTesting").show();
                $("#MenuOneTimeEventsTracker").show();
                $("#menuStopEncounter").show();
                $("#menuNewEncounter").hide();
                $("#menuPatientHome").show();
                $("#menuPatientAppointments").show();
                $("#menuScheduleAppointments").show();
                $("#menuCaptureVitals").show();
                $("#menuNewEncounter").show();
                startEncounter = 1;
                stopEncounter = 0;
                $("#menuStopEncounter").show();
                $("#menuPatientVitals").show();

            }

            /*$("#editPatientDetails").onclick(function() {
                alert("hre");
                window.location.href = "/CCC/Patient/PatientRegistration.aspx?PatientId=" + patientId;
            });*/
            }
        });
    </script>
