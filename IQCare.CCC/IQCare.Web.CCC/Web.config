        <serverProviders>
          <formatter type="System.Runtime.Remoting.Channels.BinaryClientFormatterSinkProvider, System.Runtime.Remoting" ref="Binary" typeFilterLevel="Full"/>
        </serverProviders>
      </channels>
    </application>
    <customErrors mode="on" defaultRedirect="Error.aspx"/>
